	<queryString>
		<![CDATA[select cid.nome as cidade,
ramo.nome as ramo,
#(year(now())-year(c.datanasc) as idade,
#curdate() - c.datanasc) / 365
case when (year(now())-year(c.datanasc)) <= 17 then
         '    até 17'
          when (year(now())-year(c.datanasc))  between 18 and 25 then
         'de 18 a 25'
	when  (year(now())-year(c.datanasc))  between 26 and 80 then
         'de 26 a 80'
  	when (year(now())-year(c.datanasc)) >= 81 then
         'de 81 ...'
          end as faixa_idade,
count(distinct c.id) as qtd,
sum(eq.valor) as totalinvest
from cidade cid
inner join bairro b on b.cidade_id = cid.id
inner join endereco e on e.bairro_id = b.id
inner join cidadao c ON c.endereco_id = e.id
inner join  mci_ramoempreendimento ramo ON ramo.id = c.ramoempreendimento_id
left JOIN   mci_equipamentossecretaria eq ON eq.cidadao_id = c.id

where cid.estado_id = 1
and c.benstatus = 'BENEFICIADO'
and not ISNULL(c.datanasc)
and c.anodemanda in (2011,2015)
group by
       cid.nome, faixa_idade, ramo.nome
ORDER BY
       cid.nome asc, faixa_idade, ramo.nome asc
;]]>
	</queryString>
