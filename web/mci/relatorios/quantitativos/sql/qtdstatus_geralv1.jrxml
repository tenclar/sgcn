	<queryString>
		<![CDATA[select c.tipopessoa as tipopessoa, c.statuscid  as
statuscid, c.benstatus as benstatus,
IFNULL(count(distinct c.id),0) as total,
IFNULL(sum(eq.valor),0) as totalinvest
from cidadao c
left join mci_equipamentossecretaria eq on eq.cidadao_id = c.id
inner join endereco e on e.id = c.endereco_id
inner join bairro b on b.id = e.bairro_id
inner join cidade cid on cid.id = b.cidade_id

#where cid.estado_id = 1
group by c.tipopessoa,c.benstatus, c.statuscid
order by c.tipopessoa desc, c.benstatus, c.statuscid desc;]]>
	</queryString>
