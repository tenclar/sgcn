	<queryString>
		<![CDATA[#Totalizado Por  Ano e Cidade e ramo de atividade e Total Investido
select
      cid.nome as cidade,
      c.anodemanda,
      f.nome AS equipamento,
      ramo.nome AS ramoatividade,
      eq.valor as Vr_Equip,

IFNULL(count(eq.id),0) as totalequip,
IFNULL(eq.valor,0) * IFNULL(count(eq.id),0) as totalinvest

from cidadao c
inner join mci_equipamentossecretaria eq on eq.cidadao_id = c.id
inner join endereco e on e.id = c.endereco_id
inner join bairro b on b.id = e.bairro_id
inner join mci_equipamento f on f.id = eq.equipamento_id
inner join cidade cid on cid.id = b.cidade_id

inner join  mci_ramoempreendimento ramo ON ramo.id = c.ramoempreendimento_id


where c.tipopessoa = 'CID'
and c.benstatus = 'BENEFICIADO'
and c.statuscid = 'INDIVIDUAL'

#and cid.estado_id = 1
#and ramo.nome =  'AMBULANTE' OR ramo.nome = 'DOCES E SALGADOS'
#and f.id = 12 or f.id = 13
#and c.anodemanda in (2011,2017)

and anodemanda  >= 2011
and anodemanda  <= 2016

GROUP BY

		c.anodemanda, cid.nome, ramo.nome;]]>
	</queryString>
