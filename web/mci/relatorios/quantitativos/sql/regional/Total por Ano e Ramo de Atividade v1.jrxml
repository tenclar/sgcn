	<queryString>
		<![CDATA[select
      c.anodemanda as anod,
      ramo.nome AS ramoatividade,
count(distinct c.id) as totalbenef,
sum(distinct eq.valor) as totalinvest
from cidadao c
inner join mci_equipamentossecretaria eq on eq.cidadao_id = c.id
inner join endereco e on e.id = c.endereco_id
inner join mci_ramoempreendimento ramo ON ramo.id = c.ramoempreendimento_id
inner join mpm_microcredito d ON d.id = c.id


where
     c.benstatus = 'BENEFICIADO'
     and c.anodemanda  >= 2010
     and c.anodemanda  <= 2011


GROUP BY
       ramoatividade

ORDER BY
       ramoatividade

;]]>
	</queryString>
