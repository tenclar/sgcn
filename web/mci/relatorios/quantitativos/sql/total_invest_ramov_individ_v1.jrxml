	<queryString>
		<![CDATA[select
ramo.nome as ramo,
c.anodemanda as ano,

IFNULL(count(distinct c.id),0) as qtd,
IFNULL(sum(eq.valor),0) as totalinvest

from cidade cid
inner join bairro b on b.cidade_id = cid.id
inner join endereco e on e.bairro_id = b.id
inner join cidadao c ON c.endereco_id = e.id
left JOIN   mci_equipamentossecretaria eq ON eq.cidadao_id = c.id
inner join  mci_ramoempreendimento ramo ON ramo.id = c.ramoempreendimento_id

where
c.benstatus = 'BENEFICIADO'
and c.tipopessoa = 'CID'
and c.statuscid = 'INDIVIDUAL'
and anodemanda  > 2010
and anodemanda  < 2017
group by   c.anodemanda, ramo.nome]]>
	</queryString>
