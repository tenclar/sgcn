	<queryString>
		<![CDATA[SELECT
     cidade.`nome` AS cidade_nome,
     cidadao.`nome` AS cidadao_nome,
     cidadao.`cpf` AS cidadao_cpf,
     cidadao.`numero` AS cidadao_numero,
     cidadao.`nis` AS cidadao_nis,
     cidadao.`expedidor` AS cidadao_expedidor,
     endereco.`complemento` AS endereco_complemento,
     endereco.`logradouro` AS endereco_logradouro,
     bairro.`nome` AS bairro_nome,
     bairro.`zona` AS bairro_zona,
     mci_ramoempreendimento.`nome` AS mci_ramoempreendimento_nome
FROM
     `cidadao` cidadao INNER JOIN `endereco` endereco ON cidadao.`endereco_id` = endereco.`id`
     INNER JOIN `bairro` bairro ON endereco.`bairro_id` = bairro.`id`
     INNER JOIN `cidade` cidade ON bairro.`cidade_id` = cidade.`id`
     INNER JOIN `mci_ramoempreendimento` mci_ramoempreendimento ON cidadao.`ramoempreendimento_id` = mci_ramoempreendimento.`id`
WHERE
     cidadao.tipopessoa = 'CID'
     and cidadao.benstatus = 'CADASTRO'
     and cidadao.statuscid = 'INDIVIDUAL'
GROUP BY
     cidade.nome,
     cidadao.nome
ORDER BY
     cidade.nome ASC]]>
	</queryString>
